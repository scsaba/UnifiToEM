<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:UnifiToEM.Views"
                    xmlns:vms="clr-namespace:UnifiToEM.ViewModels">

    <DataTemplate DataType="{x:Type vms:CategoryViewModel}">
        <views:CategoryView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:CategoryListViewModel}">
        <views:CategoryListView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vms:EditTransactionsViewModel}">
        <views:EditTransactionsView />
    </DataTemplate>

</ResourceDictionary>